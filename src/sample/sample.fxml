<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.effect.MotionBlur?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="198.0" prefWidth="662.0" style="-fx-background-color: skyblue;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label fx:id="textL" alignment="CENTER" prefHeight="114.0" prefWidth="662.0" style="-fx-border-color: yellow; -fx-border-width: 2px;" text="Audio Player" textAlignment="CENTER" textFill="#c45e5e">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <ProgressBar fx:id="bar" layoutY="114.0" prefHeight="18.0" prefWidth="662.0" progress="0.0">
         <effect>
            <MotionBlur />
         </effect></ProgressBar>
      <Button fx:id="play" layoutX="8.0" layoutY="142.0" mnemonicParsing="false" onAction="#play" prefHeight="34.0" prefWidth="74.0" text="Play" textFill="#0d2c1d">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="pause" layoutX="89.0" layoutY="142.0" mnemonicParsing="false" onAction="#pause" prefHeight="34.0" prefWidth="81.0" text="Pause">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <ComboBox fx:id="speed" layoutX="449.0" layoutY="147.0" onAction="#speed" prefHeight="34.0" prefWidth="99.0" promptText="Speed" style="-fx-background-color: white;" />
      <Slider fx:id="slider" layoutX="548.0" layoutY="147.0" max="100.0" prefHeight="24.0" prefWidth="114.0" value="50.0" />
      <Button fx:id="reset" layoutX="177.0" layoutY="142.0" mnemonicParsing="false" onAction="#reset" prefHeight="34.0" prefWidth="81.0" text="Reset">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="previous" layoutX="266.0" layoutY="142.0" mnemonicParsing="false" onAction="#previous" prefHeight="34.0" prefWidth="81.0" text="Prev">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="next" layoutX="353.0" layoutY="142.0" mnemonicParsing="false" onAction="#next" prefHeight="34.0" prefWidth="81.0" text="Next">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
